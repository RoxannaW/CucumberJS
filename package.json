{
  "author": "Roxanna Wijtsma",
  "dependencies": {
    "chai": "4.3.4",
    "chromedriver": "87.0.7",
    "cucumber": "6.0.5",
    "cucumber-html-reporter": "^5.3.0",
    "cucumber-pretty": "^6.0.0",
    "playwright": "1.10.0"
  },
  "description": "CucumberJS testscript using Playwright",
  "devDependencies": {
    "eslint": "7.23.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-cucumber": "1.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "keywords": [
    "chai",
    "commonjs",
    "cucumber",
    "playwright",
    "playwright automation",
    "playwright boilerplate",
    "playwright example",
    "should",
    "e2e",
    "e2e tests",
    "boilerplate",
    "integration test",
    "test automation",
    "javascript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "lint": "npx eslint --ext .js ./ --fix",
    "test": "./node_modules/.bin/cucumber-js -f node_modules/cucumber-pretty -f json:report/report.json"
  },
  "version": "1.0.0"
}
